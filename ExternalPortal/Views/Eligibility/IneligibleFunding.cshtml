
@{
    ViewData["Title"] = "Your installation may not meet the requirements to apply for payments from this scheme - Check eligibility - Green Gas Support Scheme";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <div>
            <div class="govuk-form-group">
                <fieldset class="govuk-fieldset">
                    <legend class="govuk-fieldset__legend govuk-fieldset__legend--l">
                        <h1 class="govuk-fieldset__heading">Your installation may not meet the requirements to apply for payments from this scheme</h1>
                    </legend>
                    <p>
                        You can receive payments from another scheme provided those are for a different consignment of biomethane.
                    </p>
                    <p>
                        You cannot claim for payments from two schemes for the same consignment of biomethane.
                    </p>
                    @*<p>
                        You might meet the eligibility requirements to <a href="https://www.ofgem.gov.uk/environmental-programmes">apply for financial support through one of our other schemes</a>.
                    </p>*@
                </fieldset>
            </div>
        </div>

        <div class="govuk-button-group">
            <a href="/register" role="button" draggable="false" class="govuk-button govuk-button--start govuk-!-margin-top-2 govuk-!-margin-bottom-8" data-module="govuk-button">
                Continue
                <svg class="govuk-button__start-icon" xmlns="http://www.w3.org/2000/svg" width="17.5" height="19" viewBox="0 0 33 40" role="presentation" focusable="false">
                    <path fill="currentColor" d="M0 0h13l20 20-20 20H0l20-20z"></path>
                </svg>
            </a>
        </div>
    </div>
</div>